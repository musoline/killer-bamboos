		"webpack-cli": "^4.2.0",
		"webpack-dev-server": "^3.11.0"
	},
	"dependencies": {
		"@babel/polyfill": "^7.12.1",
		"axios": "^0.21.4",
		"current-device": "^0.10.2",
		"phaser": "^3.55.2",
